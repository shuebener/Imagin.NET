<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net9.0-windows</TargetFramework>
		<UseWPF>true</UseWPF>
		<RootNamespace>Imagin.NET.Demo</RootNamespace>
		<AssemblyName>Imagin.NET.Demo</AssemblyName>
		<ApplicationIcon>Manifest.ico</ApplicationIcon>
		<Configurations>Debug;Release;Debug_Refs</Configurations>
	</PropertyGroup>

	<ItemGroup>
		<Resource Include="Images\Gear.png" />
		<Resource Include="Images\Dot.png" />
		<Resource Include="Manifest.ico" />
		<Resource Include="Images\Logo.png" />
		<Resource Include="Images\Restore.png" />
		<Resource Include="Images\UpArrow.png" />
		<Resource Include="Images\16Logo.png" />
		<Resource Include="Images\XRound.png" />
		<Resource Include="Images\Tag.png" />
		<Resource Include="Images\Target.png" />

		<None Update="App.config" CopyToOutputDirectory="PreserveNewest" />

		<None Update="Properties\Settings.settings">
		  <Generator>SettingsSingleFileGenerator</Generator>
		  <LastGenOutput>Settings.Designer.cs</LastGenOutput>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Windows.Compatibility" Version="9.0.2" />
		<PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.135" />
		<!--<PackageReference Include="Microsoft.WindowsAPICodePack-Core" Version="1.1.0.2" />-->
		<!--<PackageReference Include="Microsoft.WindowsAPICodePack-Shell" Version="1.1.0.0" />-->
		<PackageReference Include="WPFLocalizeExtension" Version="3.10.0" />
		<PackageReference Include="XAMLMarkupExtensions" Version="2.1.3" />
		
		<!--
		<package id="Microsoft.WindowsAPICodePack-Core" version="1.1.0.2" targetFramework="net46" />
		<package id="Microsoft.WindowsAPICodePack-Shell" version="1.1.0.0" targetFramework="net46" />
