		<package id="System.Windows.Interactivity.WPF" version="2.0.20525" targetFramework="net46" />
		<package id="WPFLocalizeExtension" version="3.1.0" targetFramework="net46" />
		<package id="XAMLMarkupExtensions" version="1.3.0" targetFramework="net46" />
		<package id="Microsoft.WindowsAPICodePack-Core" version="1.1.0.2" targetFramework="net46" />
		<package id="Microsoft.WindowsAPICodePack-Shell" version="1.1.0.0" targetFramework="net46" />
		<package id="System.Reflection.TypeExtensions" version="4.4.0" targetFramework="net46" requireReinstallation="true" />
